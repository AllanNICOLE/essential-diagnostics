<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <system.diagnostics>
    <sharedListeners>
      <add name="coloredconsole"
        type="Essential.Diagnostics.ColoredConsoleTraceListener, Essential.Diagnostics"
        template="{DateTime:HH':'mm':'ssZ} [{Thread}] {EventType} {Source} {Id}: {Message}{Data}"
        startColor="DarkCyan"
        stopColor="DarkCyan"
        convertWriteToEvent="true" />
      <add name="seq-basic"
        type="Essential.Diagnostics.SeqTraceListener, Essential.Diagnostics.SeqTraceListener"
        initializeData="http://localhost:5341"
        traceOutputOptions="LogicalOperationStack"
        apiKey="" />
      <add name="seq-detailed"
        type="Essential.Diagnostics.SeqTraceListener, Essential.Diagnostics.SeqTraceListener"
        initializeData="http://localhost:5341"
        additionalProperties="ThreadId,ProcessId,LogicalOperationStack,Callstack,MachineName,User"
        apiKey="" />
    </sharedListeners>
    <sources>
      <source name="SeqFrameworkDiagnostics.Example.Basic" switchValue="All">
        <listeners>
          <clear />
          <add name="coloredconsole" />
          <add name="seq-basic" />
        </listeners>
      </source>
      <source name="SeqFrameworkDiagnostics.Example" switchValue="All">
        <listeners>
          <clear />
          <add name="coloredconsole" />
          <add name="seq-basic" />
        </listeners>
      </source>
      <source name="SeqFrameworkDiagnostics.Example.Detailed" switchValue="All">
        <listeners>
          <clear />
          <add name="coloredconsole" />
          <add name="seq-detailed" />
        </listeners>
      </source>
    </sources>
    <trace>
      <listeners>
        <clear />
        <add name="coloredconsole" />
        <add name="seq-basic" />
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>