<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.diagnostics>
    <!--<trace autoflush="true" />-->
    <sources>
      <source name="Essential.Diagnostics.Tests" switchValue="All">
        <listeners>
          <clear />
          <add name="assemblyTest" />
        </listeners>
      </source>
      <source name="expressionFilter1Source" switchValue="All">
        <listeners>
          <clear />
          <add name="inmemory1">
            <filter type="ExpressionFilter" initializeData=""/>
          </add>
        </listeners>
      </source>
      <source name="inmemory1Source" switchValue="All">
        <listeners>
          <clear />
          <add name="inmemory1" />
        </listeners>
      </source>
      <source name="inmemory2Source" switchValue="All">
        <listeners>
          <clear />
          <add name="inmemory2" />
        </listeners>
      </source>
      <source name="testSource" switchValue="All">
        <listeners>
          <clear />
          <add name="test" />
        </listeners>
      </source>
      <source name="testScopeSource" switchValue="All">
        <listeners>
          <clear />
          <add name="testScopeListener" type="Essential.Diagnostics.Tests.Utility.TestTraceListener, Essential.Diagnostics.Tests.Utility"/>
          <add name="testScopeXmlWriter" type="System.Diagnostics.XmlWriterTraceListener" initializeData="testScope.svclog" />
        </listeners>
      </source>
      <source name="tracelogTestSource" switchValue="All">
        <listeners>
          <clear />
          <add name="tracelogTestListener" />
        </listeners>
      </source>
    </sources>
    <sharedListeners>
      <add name="assemblyTest"
           type="Essential.Diagnostics.Tests.Utility.TestTraceListener, Essential.Diagnostics.Tests.Utility" />
      <add name="console"
           type="System.Diagnostics.ConsoleTraceListener"
           traceOutputOptions="LogicalOperationStack" />
      <add name="inmemory1"
           type="Essential.Diagnostics.InMemoryTraceListener, Essential.Diagnostics.InMemoryTraceListener"
           initializeData="10" />
      <add name="inmemory2"
           type="Essential.Diagnostics.InMemoryTraceListener, Essential.Diagnostics.InMemoryTraceListener"
           initializeData="666" />
      <add name="test"
           type="Essential.Diagnostics.Tests.Utility.TestTraceListener, Essential.Diagnostics.Tests.Utility" />
      <add name="tracelogTestListener"
           type="Essential.Diagnostics.Tests.Utility.TestTraceListener, Essential.Diagnostics.Tests.Utility" />
    </sharedListeners>    
    <trace autoflush="true" indentsize="4" useGlobalLock="false" >
      <listeners>
        <clear />
        <add name="test" />
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>